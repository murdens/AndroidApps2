<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">

        <item name="colorPrimary">#018786</item>
        <!-- Primary theme color of the app (sets background color of app bar) -->
        <!-- Background color of buttons in the app -->
        <item name="colorButtonNormal">#018786</item>
    </style>

    <style name="textCustom">
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="fontFamily">@font/poppins_semibold</item>
    </style>
    <style name="checkCustom">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="fontFamily">@font/poppins_medium</item>
    </style>
</resources>